<%- include('partials/header') -%>

<h1> <%= homeTitle %> </h1>
<p> <%= homeStartingContent %> </p>

<% posts.forEach(function(post){ %>
<h1> <%= post.title.substring(0, 100) %> </h1>
<p> <%= post.content.substring(0, 100) %> </p>
<a href="/posts/<%= post._id %>">Read More</a>
<% }); %>

<%- include('partials/footer') -%>